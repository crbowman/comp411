/*  Example: C program to find area of a circle */

#include <stdio.h>

main()
{
  int a, b, c, d, e, f;
    
  printf("Enter six integers:\n");
  fscanf("%i%i%i%i%i%i", &a, &b, &c, &d, &e, &f);

  printf("1234567890bb1234567890\n");
  printf("%10d  %10d\n", a, b);
  printf("%10d  %10d\n", c, d);
  printf("%10d  %10d\n", e, f);
  return;
  
}


